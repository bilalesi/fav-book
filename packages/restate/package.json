{
    "name": "@favy/restate",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./src/index.ts"
        },
        "./*": {
            "types": "./dist/*.d.ts",
            "default": "./src/*.ts"
        }
    },
    "type": "module",
    "scripts": {
        "build": "tsdown",
        "dev": "bun run src/server.ts",
        "start": "node dist/server.js",
        "register": "./register-worker.sh"
    },
    "devDependencies": {
        "tsdown": "catalog:"
    },
    "peerDependencies": {
        "typescript": "^5"
    },
    "dependencies": {
        "@favy/ai": "workspace:*",
        "@favy/db": "workspace:*",
        "@favy/trigger": "workspace:*",
        "@favy/media-downloader": "workspace:*",
        "@favy/shared": "workspace:*",
        "@favy/storage": "workspace:*",
        "@restatedev/restate-sdk": "^1.3.3",
        "@restatedev/restate-sdk-clients": "^1.3.3",
        "dotenv": "catalog:",
        "zod": "catalog:"
    }
}